input_utente = input("inserisci una lista di numeri separati dalle virgole")


lista_str = input_utente.split(",")

lista_numeri = []

for i in lista_str:

    numero = int(i.strip())
    lista_numeri.append(numero)


    for numero in lista_numeri:

        quadrato = numero ** 2
        print(f"il quadrato di {numero} Ã¨ {quadrato}")